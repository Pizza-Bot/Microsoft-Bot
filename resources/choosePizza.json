{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.0",
  "body": [
    {
      "type": "TextBlock",
      "text": "Your registration is almost complete",
      "size": "medium",
      "weight": "bolder"
    },
    {
      "type": "TextBlock",
      "text": "What type of food do you prefer?",
      "wrap": true
    },
    {
      "type": "ImageSet",
      "imageSize": "medium",
      "images": [
        {
          "type": "Image",
          "url": "data:image/jpeg;base64,"
        },
        {
          "type": "Image",
          "url": "https://www.jennycancook.com/wp-content/uploads/2015/03/BBQ_Chicken_Pizza_600.jpg"
        },
        {
          "type": "Image",
          "url": "https://pizzahouse.nz/wp-content/uploads/2016/03/Veg_Delight_new.png"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Action.ShowCard",
      "title": "CapsicumPizza",
      "card": {
        "type": "AdaptiveCard",
        "body": [],
        "actions": [
          {
            "type": "Action.Submit",
            "title": "OK",
            "data": {
              "FoodChoice": "CapsicumPizza"
            }
          }
        ]
      }
    },
    {
      "type": "Action.ShowCard",
      "title": "ChickenPizza",
      "card": {
        "type": "AdaptiveCard",
        "body": [],
        "actions": [
          {
            "type": "Action.Submit",
            "title": "OK",
            "data": {
              "FoodChoice": "ChickenPizza"
            }
          }
        ]
      }
    },
    {
      "type": "Action.ShowCard",
      "title": "VegDelightPizza",
      "card": {
        "type": "AdaptiveCard",
        "body": [],
        "actions": [
          {
            "type": "Action.Submit",
            "title": "OK",
            "data": {
              "FoodChoice": "VegDelightPizza"
            }
          }
        ]
      }
    }
  ]
}
